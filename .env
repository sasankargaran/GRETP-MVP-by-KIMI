# GRETP Environment Variables

# API Configuration
VITE_API_URL=https://api.gretp.com
VITE_API_VERSION=v1

# AI Service Configuration
VITE_AI_SERVICE_URL=https://ai.gretp.com
VITE_AI_API_KEY=your_ai_api_key_here

# Maps and Geolocation
VITE_MAPS_API_KEY=your_maps_api_key_here
VITE_GEOLOCATION_SERVICE_URL=https://geo.gretp.com

# Analytics and Tracking
VITE_ANALYTICS_ID=your_analytics_id_here
VITE_TRACKING_ENABLED=true

# Feature Flags
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_VIRTUAL_TOURS=false
VITE_ENABLE_MORTGAGE_CALCULATOR=false

# Performance Settings
VITE_ENABLE_PWA=false
VITE_SERVICE_WORKER_ENABLED=false

# External Services
VITE_PAYMENT_GATEWAY_URL=https://payments.gretp.com
VITE_NOTIFICATION_SERVICE_URL=https://notifications.gretp.com

# Development Settings
VITE_MOCK_DATA_ENABLED=true
VITE_DEBUG_MODE=false